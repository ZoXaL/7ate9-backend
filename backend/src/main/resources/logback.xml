<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%thread] %-5level %logger{35} - %msg %n</pattern>
        </encoder>
    </appender>

    <root level="NONE">
        <!--<appender-ref ref="STDOUT" />-->
    </root>

    <logger name="com.yatty.sevennine" level="debug" additivity="false">
        <appender-ref ref="STDOUT" />
    </logger>
</configuration>